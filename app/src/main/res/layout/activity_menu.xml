<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu">

    <ImageButton
        android:id="@+id/AfficherCA"
        android:layout_width="134dp"
        android:layout_height="117dp"
        android:layout_marginBottom="236dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/ListeVehiculeDispo"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageButton
        android:id="@+id/ListeVehiculeDispo"
        android:layout_width="134dp"
        android:layout_height="117dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="88dp"
        app:layout_constraintBottom_toTopOf="@+id/AfficherCA"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.646"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageButton
        android:id="@+id/ListeVehiculeLoue"
        android:layout_width="134dp"
        android:layout_height="117dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/ListeVehiculeDispo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ListeVehiculeDispo"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageButton
        android:id="@+id/RestitutionVehicule"
        android:layout_width="134dp"
        android:layout_height="117dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/AfficherCA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/AfficherCA"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@mipmap/ic_launcher_round" />

</androidx.constraintlayout.widget.ConstraintLayout>